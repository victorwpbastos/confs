{
	"editor.detectIndentation": false,
	"editor.fontSize": 16,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.showFoldingControls": "always",
	"editor.snippetSuggestions": "top",
	"editor.tabSize": 4,
	"emmet.syntaxProfiles": {
		"vue": "html",
		"vue-html": "html"
	},
	"emmet.triggerExpansionOnTab": true,
	"eslint.alwaysShowStatus": true,
	"eslint.options": {
		"extensions": [".html", ".js", ".jsx", ".ts", ".vue"]
	},
	"eslint.validate": [
		{ "autoFix": true, "language": "html" },
		{ "autoFix": true, "language": "javascript" },
		{ "autoFix": true, "language": "javascriptreact" },
		{ "autoFix": true, "language": "typescript" },
		{ "autoFix": true, "language": "vue" }
	],
	"explorer.openEditors.visible": 0,
	"files.eol": "\n",
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"prettier.disableLanguages": [""],
	"prettier.eslintIntegration": true,
	"prettier.endOfLine": "lf",
	"prettier.arrowParens": "avoid",
	"prettier.bracketSpacing": true,
	"prettier.printWidth": 120,
	"prettier.quoteProps": "as-needed",
	"prettier.semi": true,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.trailingComma": "none",
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"terminal.integrated.cursorStyle": "block",
	"terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
	"vetur.completion.autoImport": true,
	"vetur.format.options.tabSize": 4,
	"vetur.format.options.useTabs": true,
	"vetur.format.scriptInitialIndent": true,
	"vetur.format.styleInitialIndent": true,
	"whiteviz.expandedTabIndicator": true,
	"whiteviz.visualizeEOL": false,
	"workbench.colorTheme": "Noctis Minimus",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.statusBar.feedback.visible": false,
	"workbench.tips.enabled": false,
	"workbench.iconTheme": "file-icons"

	// "editor.formatOnSave": false,
	// "editor.formatOnPaste": false,
	// "terminal.integrated.shellArgs.windows": [
	// 	"-NoExit",
	// 	"Set-PSReadLineOption",
	// 	"â€“HistoryNoDuplicates:$True"
	// ],
	// "eslint.autoFixOnSave": true,
}
